<div class="container" ng-app="storageApp" ng-controller="StorageDetailCtrl">
    <div class="row">
        <div class="col-lg-3">
            <label>{[{result}]}</label>
            <br>
            <lable>{{error}}</lable>
        </div>
        <div class="col-lg-8">
            <h2>{{{__ "Line Detail"}}}</h2>
        </div>

    </div>

    <div class="row">

        <div class="col-lg-12">
            <input id="stoage" type="text" class="hidden" value="{{stoage}}"/>
            <div>
                <form class="form-horizontal" role="form">
                    <div class="form-group">
                        <label class="col-md-2 control-label">{{{__ "Ident"}}}</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" name="Ident"
                                   ng-model="stoage.Ident" />
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">{{{__ "Name"}}}</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" name="Name"
                                   ng-model="stoage.Name"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="col-md-2 control-label">{{{__ "Category"}}}</label>
                        <div class="col-md-4">
                            <input type="text" class="form-control" name="Category"
                                   ng-model="stoage.Category"/>
                        </div>
                    </div>
                    <div class="form-group">

                        <div style="padding-left:300px">

                            <input type="submit" value="Submit" class="btn btn-primary"/>
                        </div>
                    </div>
                </form>


            </div>

        </div>
    </div>
    <div class="row">
        <div class="col-lg-8">
            <!--showing message-->
        </div>
        <div class="col-lg-4">
            <!--tools buttons-->
        </div>
    </div>
    <div class="row">
        <div class="col-lg-3">
            <!--navigation buttons-->
        </div>
        <div class="col-lg-9">
            <!--visualization-->
        </div>
    </div>
</div>
{{#section 'jquery'}}
    <script src="/js/app/storageApp.js"></script>

{{/section}}